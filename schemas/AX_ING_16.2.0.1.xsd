  <xsd:complexType name="HuidigObjectEntiteitType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="CodeObject" type="CodeObjectType2">
        <xsd:annotation>
          <xsd:documentation>Indicatie betreffende het gebruiksdoel van het onderpand.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="StraatNaam" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>Straatnaam, indien sprake is van een correspondentie adres kan ook de aanduiding 'postbus' gebruikt worden.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="HuisNr" type="N5Type">
        <xsd:annotation>
          <xsd:documentation>Huisnummer, indien sprake is van een correspondentie adres kan dit ook een postbusnummer zijn.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="HuisNrToevoeging" type="AN10Type">
        <xsd:annotation>
          <xsd:documentation>Huisnummertoevoeging, bijvoorbeeld I, Huis, zwart etc..</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Postcode" type="PostcodeType">
        <xsd:annotation>
          <xsd:documentation>Postcode, behorend bij ingevulde straatnaam huisnummer of bij ingevuld postbusnummer. Voor Nederland 6 posities lang, zonder spatie ertussen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PlaatsNaam" type="AN35Type">
        <xsd:annotation>
          <xsd:documentation>Geeft de plaatsnaam.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Gemeente" type="AN30Type">
        <xsd:annotation>
          <xsd:documentation>Geeft de gemeentenaam.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Land" type="LandType">
        <xsd:annotation>
          <xsd:documentation>ISO landcode (ISO-3166)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TeVerkopen" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft weer of het huidig object wordt verkocht of in eigendom blijft.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="VerkochtJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft weer of het huidig object is verkocht.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerkoopakteJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of er een getekende verkoopakte van het huidig object aanwezig is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerkoopPrijs" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>De (verwachte) verkoopprijs van het te verkopen object.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MarktWaarde" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>De (getaxeerde) marktwaarde van het huidig object.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerkoopDt" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>De leveringsdatum conform het verkoopcontract.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CodeVerhuur" type="VerhuurType">
        <xsd:annotation>
          <xsd:documentation>Definieert het eventuele verhuurcontract van het huidig object.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Huurinkomsten" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>De jaarlijkse huurinkomsten van het huidig onderpand.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OverbruggingJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of er sprake is van een overbruggingskrediet bij de geldgever waar de financiering aangevraagd wordt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="OverbruggingsBedrag" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Bedrag dat benodigd is als overbruggingskrediet. Het veld OverbruggingJN geeft aan of er ook een overbrugging aangevraagd wordt bij de geldverstrekker waar de AX voor bedoeld is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="HuidigeFinancieringJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of er een bestaande hypotheek is voor het object.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AanvragerBestaandeHypotheek" type="REFPartijNAWDataType">
        <xsd:annotation>
          <xsd:documentation>Geeft de naam van de entiteit waarin NAW gegevens van deze partij zijn opgenomen</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="9" name="HuidigeFinanciering" type="HuidigeFinancieringEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Entiteit met gegevens van de huidige financiering op het huidige object.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
    <xsd:attribute name="ID" type="xsd:ID"/>
  </xsd:complexType>
