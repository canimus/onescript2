  <xsd:element name="OfferteAanvraag" type="OfferteAanvraagType"/>
  <xsd:complexType name="OfferteAanvraagType">
    <xsd:sequence>
      <xsd:element name="Header" type="HeaderEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Header</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="9" name="HuidigObject" type="HuidigObjectEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Entiteit met gegevens van het huidig object en de bijbehorende financiering.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="9" name="Hypotheekgever" type="HypotheekgeverEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Entiteit met gegevens van alle personen die een hypotecaire overeenkomst aangaan met de geldverstrekker. (bijvoorbeeld ook medeschuldenaren).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="99" name="PartijNAWData" type="PartijNAWDataEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Entiteit met NAW gegevens van een partij.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="3" name="Notaris" type="NotarisEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Entiteit met gegevens van de notaris. Alleen vullen indien bekend is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TussenPersoon" type="TussenPersoonEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Entiteit met gegevens van de tussenpersoon die de aanvraag indient. Eventuele gevens van een subagent, onderliggend kantoor e.d. kunnen in TweedeTussenpersoon gevuld worden.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Object" type="ObjectEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Entiteit met gegevens van het object waar de offerteaanvraag/ aangevraagde financiering op van toepassing is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Lening" type="LeningEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Entiteit met gegevens van de aan te vragen financiering.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="9" name="FinancieleDekking" type="FinancieleDekkingEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Dit is een financiele dekking die niet gekoppeld is aan een specifiek leningdeel en over alle leningdelen heen geldt als benodigde zekerheidsstelling.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TekstRegels" type="TekstRegelsEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Let op gebruik van deze entiteit kan tot vertraging leiden bij de ontvanger. Entiteit maakt per 1-12-2010 geen onderdeel meer uit van generieke set.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="HeaderEntiteitType">
    <xsd:sequence>
      <xsd:element name="VerzenderNrHDN" type="N6Type">
        <xsd:annotation>
          <xsd:documentation>Unieke HDN-nummer van de verzender. Nummer moet overeenkomen met actieve certificaatnummer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="VerzenderNaam" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>Naam van verzendende partij zoals geregistreerd bij de ontvanger.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OntvangerNrHDN" type="N6Type">
        <xsd:annotation>
          <xsd:documentation>HDN-nummer van de ontvangende partij.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OntvangerNaam" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>(Eind)Ontvangernaam waarvoor dit bericht is opgemaakt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BerichtSoort" type="BerichtSoortType">
        <xsd:annotation>
          <xsd:documentation>Geeft de berichtsoort aan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BerichtVersie" type="N2_1Type">
        <xsd:annotation>
          <xsd:documentation>Geeft het versienummer van de generieke bericht specificaties weer waarop het bericht gebaseerd is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OntvangerCode" type="OntvangerCodeType">
        <xsd:annotation>
          <xsd:documentation>Code van de ontvanger die is aangesloten op het HDN netwerk en die gebruikt wordt voor het aanroepen van het juiste schema.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="VerzendDt" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Datum waarop het bericht verzonden is. Datum wordt gezet door applicatie die het bericht heeft aangemaakt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="VerzendTijd" type="TijdType">
        <xsd:annotation>
          <xsd:documentation>Tijd waarop het bericht verzonden is. Tijd wordt gezet door applicatie die het bericht heeft aangemaakt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AanvraagVolgNr" type="AN255Type">
        <xsd:annotation>
          <xsd:documentation>Volgnummer van de aanvraag, waarop het bericht betrekking heeft. Niet overschrijven door ontvangende partij. Nummer dient overgenomen te worden in retourberichten.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AanvraagVersie" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Geeft de versie van de aanvraag aan. Niet overschrijven door ontvangende partij. Nummer dient overgenomen te worden in retourberichten.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PakketNaam" type="AN35Type">
        <xsd:annotation>
          <xsd:documentation>Geeft de naam van het pakket dat de berichten aanmaakt of doorzend. Niet overschrijven door eindontvanger.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PakketVersie" type="AN35Type">
        <xsd:annotation>
          <xsd:documentation>Geeft de versie van het tussenpersoonpakket dat de berichten aanmaakt. Niet overschrijven door eindontvanger.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="optional"/>
  </xsd:complexType>
  <xsd:simpleType name="N6Type">
    <xsd:restriction base="xsd:nonNegativeInteger">
      <xsd:pattern value="\d{1,6}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AN100Type">
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="100"/>
