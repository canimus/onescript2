  <xsd:complexType name="HuidigeFinancieringEntiteitType">
    <xsd:sequence>
      <xsd:element name="AflossenJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de totale financiering afgelost wordt, en vervangen wordt door de nieuwe financiering (entiteit Lening en Leningdeel)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FinancierBestaandeHyp" type="MaatschappijType">
        <xsd:annotation>
          <xsd:documentation>De financier van de bestaande hypotheek.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="OmschrFinancier" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>Naam van de huidige financier. Mag alleen gebruikt worden indien de omschrijving niet in de keuzelijst voorkomt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SaldoBestaandeHyp" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het totaal openstaand saldo van de bestaande hypotheek op passeerdatum van de nieuwe hypotheek (zowel Box1 als Box3).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AflossingsvrijDeel" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geeft het deel van de totale hypotheek (alle leningdelen) waarvoor geen aflossingcomponent (financiële of bancaire dekking, annuïteit of lineair) is opgenomen in de totale huidige financiering. Peildatum is de (vermoedelijke) passeerdatum.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="OorsprHoofdsom" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>De totale oorspronkelijke hoofdsom van de bestaande hypotheek bij het aangaan van de hypotheek (zowel Box 1 als Box 3).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BedragInschrijving" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het inschrijvingsbedrag van de bestaande hypotheken inclusief eventueel verhoogde inschrijvingen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="NrBestaandeHyp" type="AN20Type">
        <xsd:annotation>
          <xsd:documentation>Het nummer van de bestaande hypotheek zoals dat in de administratie van de geldgever voorkomt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="OorspronkelijkeIngDt" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Oorspronkelijke ingangsdatum van de huidige hypotheek</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EigenWonSchuld" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het bedrag van de huidige hypotheek waarover de rente fiscaal aftrekbaar is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Garantie" type="GarantieType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de huidige financiering met NHG, een gemeentegarantie of zonder garantie is gesloten.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="RangOrde" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Geeft de rangorde van de inschrijving weer bij het kadaster.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="9" name="HuidigLeningdeel" type="HuidigLeningdeelEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Geeft een gespecificeerde leningdeel van de huidige financiering. Alleen leningdelen opgeven die niet afgelost worden en derhalve meetellen voor de toetsing.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
