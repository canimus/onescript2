  <xsd:complexType name="DatumType">
    <xsd:sequence>
      <xsd:element name="Dag">
        <xsd:simpleType>
          <xsd:annotation>
            <xsd:documentation>Het veld dag wordt gevuld met een waarde van minimaal 1 en maximaal 31</xsd:documentation>
          </xsd:annotation>
          <xsd:restriction base="xsd:nonNegativeInteger">
            <xsd:maxInclusive value="31"/>
            <xsd:minInclusive value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="Maand">
        <xsd:simpleType>
          <xsd:annotation>
            <xsd:documentation>Het veld maand wordt gevuld met een waarde van minimaal 1 en maximaal 12</xsd:documentation>
          </xsd:annotation>
          <xsd:restriction base="xsd:nonNegativeInteger">
            <xsd:maxInclusive value="12"/>
            <xsd:minInclusive value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="Jaar">
        <xsd:simpleType>
          <xsd:annotation>
            <xsd:documentation>Jaartal, bijvoorbeeld 2006</xsd:documentation>
          </xsd:annotation>
          <xsd:restriction base="xsd:nonNegativeInteger">
            <xsd:pattern value="\d{4}"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TijdType">
    <xsd:sequence>
      <xsd:element name="Uur">
        <xsd:simpleType>
          <xsd:annotation>
            <xsd:documentation>Het veld uur wordt gevuld met een waarde van minimaal 0 en maximaal 23</xsd:documentation>
          </xsd:annotation>
          <xsd:restriction base="xsd:nonNegativeInteger">
            <xsd:maxInclusive value="23"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="Minuten">
        <xsd:simpleType>
          <xsd:annotation>
            <xsd:documentation>Het veld minuten wordt gevuld met een waarde van minimaal 0 en maximaal 59</xsd:documentation>
          </xsd:annotation>
          <xsd:restriction base="xsd:nonNegativeInteger">
            <xsd:maxInclusive value="59"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="Seconden">
        <xsd:simpleType>
          <xsd:annotation>
            <xsd:documentation>Het veld seconden wordt gevuld met een waarde minimaal 0 en maximaal 59</xsd:documentation>
          </xsd:annotation>
          <xsd:restriction base="xsd:nonNegativeInteger">
            <xsd:maxInclusive value="59"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
