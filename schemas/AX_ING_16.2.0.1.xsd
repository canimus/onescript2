  <xsd:complexType name="HuidigLeningdeelEntiteitType">
    <xsd:sequence>
      <xsd:element name="AflossenJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan het leningdeel afgelost wordt, en vervangen wordt door de nieuwe financiering.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AflossingsVormHuidigLeningdeel" type="AflossingsVormType">
        <xsd:annotation>
          <xsd:documentation>Geeft de wijze van aflossing van het huidige leningdeel.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RenteVastInMndHuidigLeningdeel" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Aantal maanden waarin de rente contractueel is vastgelegd, als totaal inclusief mogelijke rentebedenktijdperiodes van het huidige leningdeel.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DuurInMndHuidigLeningdeel" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Geeft de oorspronkelijke looptijd van het leningdeel in maanden, 999 = levenslang.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BedragHuidigLeningdeel" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geeft het bedrag van een afzonderlijk huidig leningdeel.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RestantSaldoHuidigLeningdeel" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het actuele saldo van het huidige leningdeel op de (vermoedelijke) passeerdatum</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RentePctHuidigLeningdeel" type="N11_3Type">
        <xsd:annotation>
          <xsd:documentation>Geeft het huidige rentepercentage van het huidige leningdeel weer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="AflossingsVormType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Annuiteit"/>
      <xsd:enumeration value="02 Lineair"/>
      <xsd:enumeration value="03 Levensverzekering"/>
      <xsd:enumeration value="05 Aflossingsvrij"/>
      <xsd:enumeration value="09 Krediet of bankhypotheek"/>
      <xsd:enumeration value="10 Beleggingshypotheek"/>
      <xsd:enumeration value="13 Spaarrekening"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="N11_3Type">
    <xsd:restriction base="xsd:decimal">
      <xsd:pattern value="-?\d{1,11}\.\d{3}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="HypotheekgeverEntiteitType">
    <xsd:sequence>
      <xsd:element name="RefPartijNAWData" type="REFPartijNAWDataType">
        <xsd:annotation>
          <xsd:documentation>Geeft de naam/het nummer van de entiteit waarin de NAW gegevens van de hypotheekgever zijn opgenomen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TelefoonNrPrive" type="TelefoonNummerType">
        <xsd:annotation>
          <xsd:documentation>Telefoonnummer waarop de hypotheekgever privé te bereiken is. (Noteren zonder koppeltekens e.d)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TelefoonNrWerk" type="TelefoonNummerType">
        <xsd:annotation>
          <xsd:documentation>Telefoonnummer waarop de hypotheekgever tijdens kantooruren te bereiken is. (Noteren zonder koppeltekens e.d)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TelefoonNrMobiel" type="TelefoonNummerType">
        <xsd:annotation>
          <xsd:documentation>Telefoonnummer waarop de hypotheekgever mobiel te bereiken is. (Noteren zonder koppeltekens e.d)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="E-mailadres" type="EmailType">
        <xsd:annotation>
          <xsd:documentation>E-mail adres van de hypotheekgever.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BurgerlijkeStaat" type="BurgerlijkeStaatType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan wat de burgerlijke staat van de hypotheekgever is op (vermoedelijke) passeerdatum.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="HuwelijkOntbonden" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of een eerder huwelijk en/of geregistreerd partnerschap van de hypotheekgever is/zijn ontbonden door echtscheiding.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="HuwelijkOntbondenDt" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>De datum van ontbinding van het laatste huwelijk/gereg. Partnerschap.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="WeduweWeduwnaar" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de hypotheekgever weduwe/weduwnaar is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="OpleidingsNiveau" type="OpleidingsNiveauType">
        <xsd:annotation>
          <xsd:documentation>Het niveau van de hoogst genoten opleiding van de hypotheekgever.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="HoofdAanspr" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de hypotheekgever hoofdelijk aansprakelijk is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EersteHypotheekOoit" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of dit de eerste hypotheek van de hypotheekgever is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EigenWonReserve" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het bedrag waarvoor de klant geen eigenwoningschuld kan aangaan als hij een andere woning koopt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AfwKlantProfiel" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de aanvraag van het advies afwijkt van het gemaakt klantprofiel.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="KlantSegmentMij" type="KlantSegmentMijType">
        <xsd:annotation>
          <xsd:documentation>Geeft het maatschappijspecifieke klantsegment aan waartoe de klant behoort bij de maatschappij/geldgever waar de aanvraag voor is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Inkomsten" type="InkomstenEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Geeft alle inkomstenbronnen weer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="9" name="Verplichtingen" type="VerplichtingenEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Entiteit met gegevens van de verplichtingen van de partij (consumptieve kredieten, alimentatie e.d.) Ook hypotheken zonder object dienen hier opgegeven te worden.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="9" name="BezittingenBoxDrie" type="BezittingenBoxDrieEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Gegevens van alle bezittingen in Box 3. Indien er inkomen uit dit vermogen meegenomen dient te worden, dan dient dit opgenomen te worden in entiteit Vermogen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Identificatie" type="IdentificatieEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Identificatie</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="TelefoonNummerType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="0{1,2}[0-9]{8,23}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="EmailType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Za-z0-9_!#$&amp;'*+\\/=\?`\{\}~\^\.-]{1,190}@[A-Za-z0-9.-]{1,64}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="BurgerlijkeStaatType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Gehuwd (gemeenschap van goederen)"/>
      <xsd:enumeration value="02 Gehuwd met huwelijkse voorwaarden"/>
      <xsd:enumeration value="03 Alleenstaand"/>
      <xsd:enumeration value="04 Samenwonend met samenlevingscontract"/>
      <xsd:enumeration value="05 Samenwonend zonder samenlevingscontract"/>
      <xsd:enumeration value="06 Partnerregistratie in gemeenschap van goederen"/>
      <xsd:enumeration value="07 Partnerregistratie onder registratievoorwaarden"/>
      <xsd:enumeration value="08 Gehuwd volgens buitenlands recht"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="OpleidingsNiveauType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Lager onderwijs"/>
      <xsd:enumeration value="02 Voortgezet onderwijs (MAVO/HAVO)"/>
      <xsd:enumeration value="03 VWO"/>
      <xsd:enumeration value="04 Lager beroepsonderwijs"/>
      <xsd:enumeration value="05 Middelbaar beroepsonderwijs"/>
      <xsd:enumeration value="06 Hoger beroepsonderwijs"/>
      <xsd:enumeration value="07 (post)Academisch"/>
      <xsd:enumeration value="08 Onbekend"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="KlantSegmentMijType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="IN005 Eigen personeel"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="InkomstenEntiteitType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="LandBelastingVerplichting" type="LandType">
        <xsd:annotation>
          <xsd:documentation>Land waarin belastingverplichting geldt voor alle onderliggende inkomsten.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="9" name="DienstBetrekking" type="DienstBetrekkingEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Entiteit met de inkomensgegevens uit dienstbetrekking.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="9" name="Onderneming" type="OndernemingEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Entiteit met de inkomensgegevens uit onderneming (ZZP, 1 manszaak, VOF enz).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="9" name="Uitkering" type="UitkeringEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Entiteit met inkomen uit uitkering (Sociaal, privaat of collectief).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="9" name="Pensioen" type="PensioenEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Entiteit met inkomensgegevens (exclusief AOW) uit pensioen (Ouderdomspensioen, VUT, PrePensioen enz).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="9" name="Overige" type="OverigeEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Entiteit met overige inkomsten die niet in andere entiteiten voorkomen (alimentatie enz).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="9" name="Vermogen" type="VermogenEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Entiteit met inkomensgegevens uit vermogen (inkomen uit beleggen, sparen verhuur enz). Indien er wel vermogen is, maar geen (specifiek) inkomen uit dat vermogen dan dient de entiteit BezittingenBoxDrie gebruikt te worden.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="optional"/>
  </xsd:complexType>
  <xsd:complexType name="DienstBetrekkingEntiteitType">
    <xsd:sequence>
      <xsd:element name="SoortDienstBetrekking" type="SoortDienstBetrekkingType">
        <xsd:annotation>
          <xsd:documentation>Code die aangeeft wat de soort dienstbetrekking is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="FlexWerkerJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de aanvraag wordt gedaan op basis van de flexwerkersregeling.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DGAJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de inkomsten uit het dienstverband te kenmerken zijn als DGA inkomsten.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Beroep" type="BeroepType">
        <xsd:annotation>
          <xsd:documentation>De omschrijving van het beroep van de hypotheekgever. Let op! maakt vanaf versie 10 geen onderdeel meer uit van de generieke set.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="IngangsDtDienstBetrekking" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Geeft de ingangsdatum van de dienstbetrekking.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ProeftijdJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of er sprake is van een proeftijd.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ProeftijdVerstrekenJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de proeftijd verstreken is. Datum werkgeversverklaring is leidend.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EindDtDienstBetrekking" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Geeft de datum waarop de dienstbetrekking in de huidige vorm beëindigd zal worden. Ook vullen indien er voornemens zijn het dienstverband te beeindigen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Werkgever" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>De naam van de werkgever.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="StraatNaamWerkgever" type="AN35Type">
        <xsd:annotation>
          <xsd:documentation>Straatnaam van de werkgever, indien sprake is van een correspondentie adres kan ook de aanduiding "postbus" gebruikt worden.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="HuisNrWerkgever" type="N5Type">
        <xsd:annotation>
          <xsd:documentation>Het huisnummer, indien sprake is van een correspondentie adres kan dit ook een postbusnummer zijn.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="HuisNrToevoegingWerkgever" type="AN10Type">
        <xsd:annotation>
          <xsd:documentation>Huisnummertoevoeging, bijvoorbeeld I, Huis, zwart etc.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PostcodeWerkgever" type="AN9Type">
        <xsd:annotation>
          <xsd:documentation>De postcode, behorend bij ingevulde straatnaam, huisnummer of bij ingevuld postbusnummer. Voor Nederland 6 posities lang, zonder spatie ertussen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PlaatsNaamWerkgever" type="AN30Type">
        <xsd:annotation>
          <xsd:documentation>De plaatsnaam van de werkgever.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="LandWerkgever" type="LandType">
        <xsd:annotation>
          <xsd:documentation>Land waar de werkgever gevestigd is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="LandBelastingVerplichting" type="LandType">
        <xsd:annotation>
          <xsd:documentation>Land waarin belastingverplichting geldt op de gespecificeerde dienstbetrekking.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="GemUrenWeek" type="N2Type">
        <xsd:annotation>
          <xsd:documentation>Gemiddeld aantal uren werkzaam in deze dienstbetrekking per week.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BrutoJaarSalaris" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het bruto basissalaris excl. overwerk, vakantietoeslag, provisie enz. Het brutojaarsalaris van het gebruikelijke aantal werkwerken in de bedrijfstak.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="VakantieToeslag" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Gemiddeld bedrag aan vakantiegeld op jaarbasis.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="OnregelmatigheidsToeslag" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Onregelmatigheidstoeslag op jaarbasis (mits structureel van aard).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Vaste13eMnd" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geeft het bedrag aan vaste 13e maand. Onder vast wordt verstaan: In de arbeidsovereenkomst vastgelegde onvoorwaardelijke inkomensbestanddelen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ProvisiePerJaar" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het bedrag aan provisie op jaarbasis (mits structureel van aard) Bedrag van laatste 12 maanden.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VasteEindejaarsuitkering" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Bedrag aan vaste eindejaarsuitkering. Onder vast wordt verstaan: In de arbeidsovereenkomst vastgelegde onvoorwaardelijke inkomensbestanddelen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="OverwerkPerJaar" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het bedrag aan overwerkvergoeding (jaartotaal), mits structureel van aard. Bedrag van de afgelopen 12 maanden.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BrutoJaarInkVariabel" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het variabele inkomen zonder structurele componenten exclusief provisie, onregelmatigheidstoeslag, overwerk en vaste eindejaarsuitkering en dergelijke.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VEB" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>VEB (Vergoeding voor Extra Beslaglegging) toelage voor militairen bedrag van de afgelopen 12 maanden.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="InkomstenToeslag" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geeft de inkomsten uit toeslagen die structrureel mee kunnen tellen (bijv.diplomatoeslag, gevarentoeslag, oefeningtoeslag enz.)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PctInkomenMeetellen" type="N11_3Type">
        <xsd:annotation>
          <xsd:documentation>Geeft het percentage van het inkomen dat meegeteld moet worden ter bepaling van de leencapaciteit. Alleen opgeven als afwijkt van 100%.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DuurInMndMeetellen" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Geeft de specifieke looptijd in maanden waar een percentage van het inkomen moet meegeteld worden ter bepaling van de leencapaciteit. Alleen opgeven indien niet gelijk is aan duur hypotheek.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="InkomenPeriode" type="InkomenPeriodeType">
        <xsd:annotation>
          <xsd:documentation>Geef aan wat de uitbetaaltermijn is van het inkomen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="LoonbeslagJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of er loonbeslag is gelegd op het inkomen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="InkomenFlexibel1jr" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Totaal flexibel jaarinkomen van het eerste voorgaande jaar.(huidig jaar – 1jr) Hier wordt het inkomen bedoeld zoals beschreven de normen van NHG inzake flexibele en overige arbeidsrelaties.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="InkomenFlexibel2jr" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Totaal flexibel jaarinkomen van het tweede voorgaande jaar. (huidig jaar – 2jr) Hier wordt het inkomen bedoeld zoals beschreven de normen van NHG inzake flexibele en overige arbeidsrelaties.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="InkomenFlexibel3jr" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Totaal flexibel jaarinkomen van het derde voorgaande jaar.(huidig jaar – 3jr) Hier wordt het inkomen bedoeld zoals beschreven de normen van NHG inzake flexibele en overige arbeidsrelaties.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
