  <xsd:complexType name="OndernemingEntiteitType">
    <xsd:sequence>
      <xsd:element name="RechtsVorm" type="RechtsVormType">
        <xsd:annotation>
          <xsd:documentation>Geeft het type rechtvorm van de onderneming aan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SoortOnderneming" type="SoortOndernemingType">
        <xsd:annotation>
          <xsd:documentation>Geeft de soort onderneming aan behorende bij de gekozen rechtsvorm.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="IngangsDtOnderneming" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Geeft de datum van de start van de onderneming aan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="KvKnummer" type="N8Type">
        <xsd:annotation>
          <xsd:documentation>Het nummer van de onderneming waarmee het bij de Kamer van Koophandel geregistreerd staat.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Inkomenzelfstandige" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>De gemiddelde netto jaarwinst over de afgelopen 3 kalenderjaren (tot maximaal de nettowinst van het laatste kalenderjaar).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Inkomen1jr" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Totaal jaarinkomen van het eerste voorgaande kalenderjaar. Hier wordt het inkomen bedoeld zoals beschreven de normen van NHG inzake inkomen uit zelfstandig beroep of bedrijf.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Inkomen2jr" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Totaal jaarinkomen van twee kalenderjaren terug. Hier wordt het inkomen bedoeld zoals beschreven de normen van NHG inzake inkomen uit zelfstandig beroep of bedrijf.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Inkomen3jr" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Totaal jaarinkomen van drie kalenderjaren terug. Hier wordt het inkomen bedoeld zoals beschreven de normen van NHG inzake inkomen uit zelfstandig beroep of bedrijf.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PctInkomenMeetellen" type="N11_3Type">
        <xsd:annotation>
          <xsd:documentation>Geeft het percentage van het inkomen dat meegeteld moet worden ter bepaling van de leencapaciteit. Alleen opgeven als afwijkt van 100%.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DuurInMndMeetellen" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Geeft de specifieke looptijd in maanden waar een percentage van het inkomen moet meegeteld worden ter bepaling van de leencapaciteit. Alleen opgeven indien niet gelijk is aan duur hypotheek.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
    <xsd:attribute name="ID" type="xsd:ID"/>
  </xsd:complexType>
  <xsd:simpleType name="RechtsVormType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Eenmanszaak"/>
      <xsd:enumeration value="02 Maatschap"/>
      <xsd:enumeration value="03 Vennootschap onder firma (VOF)"/>
      <xsd:enumeration value="04 Commanditaire vennootschap (CV)"/>
      <xsd:enumeration value="05 Besloten vennootschap (BV)"/>
      <xsd:enumeration value="06 Naamloze vennootschap (NV)"/>
      <xsd:enumeration value="07 Vereniging"/>
      <xsd:enumeration value="08 Stichting"/>
      <xsd:enumeration value="09 Coöperatie met beperkte aansprakelijkheid (BA)"/>
      <xsd:enumeration value="10 Coöperatie met uitgesloten aansprakelijkheid (UA)"/>
      <xsd:enumeration value="11 Coöperatie met wettelijke aansprakelijkheid (WA)"/>
      <xsd:enumeration value="12 Onderlinge waarborgmaatschappij"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="SoortOndernemingType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Zelfstandige Zonder Personeel (ZZP)"/>
      <xsd:enumeration value="03 Zelfstandige met personeel"/>
      <xsd:enumeration value="05 Freelance"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="N8Type">
    <xsd:restriction base="xsd:nonNegativeInteger">
      <xsd:pattern value="\d{1,8}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="UitkeringEntiteitType">
    <xsd:sequence>
      <xsd:element name="SoortUitkering" type="SoortUitkeringType">
        <xsd:annotation>
          <xsd:documentation>Geeft het soort uitkering aan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AOSoort" type="AoSoortType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de arbeidsongeschiktheidsuitkering vast of tijdelijk is. Voor tijdelijk kan gelezen worden herkeurbaar of herbeoordeelbaar. Voor vast geldt dat deze niet herkeurbaar of herbeoordeelbaar is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SoortVerzekering" type="SoortVerzekeringType">
        <xsd:annotation>
          <xsd:documentation>Geeft het soort verzekering weer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BrutoJaarUitkering" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>De bruto basisuitkering excl. vakantietoeslag enz. Het brutojaarbedrag van gebruikelijke aantal uitkeringsweken behorende bij de uitkering.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="VakantieToeslag" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Gemiddeld bedrag aan vakantiegeld op jaarbasis.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="IngangsDtUitkering" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Geeft de ingangsdatum van de uitkering aan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EindDtUitkering" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Geeft de einddatum van de uitkering aan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VolledigeAOWOpbouwJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of er sprake is van een volledige AOW opbouw. (Wel of geen korting op AOW i.v.m. woonachtig zijn buiten Nederland).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AantalAOWJaren" type="N2Type">
        <xsd:annotation>
          <xsd:documentation>Geeft aan hoeveel jaar AOW er maximaal opgebouwd kan worden tot de AOW leeftijd.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="LandBelastingVerplichting" type="LandType">
        <xsd:annotation>
          <xsd:documentation>Land waarin belastingverplichting geldt op de gespecificeerde inkomsten uit uitkering.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PctInkomenMeetellen" type="N11_3Type">
        <xsd:annotation>
          <xsd:documentation>Geeft het percentage van het inkomen dat meegeteld moet worden ter bepaling van de leencapaciteit. Alleen opgeven als afwijkt van 100%.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DuurInMndMeetellen" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Geeft de specifieke looptijd in maanden waar een percentage van het inkomen moet meegeteld worden ter bepaling van de leencapaciteit. Alleen opgeven indien niet gelijk is aan duur hypotheek.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="SoortUitkeringType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 WIA"/>
      <xsd:enumeration value="02 Bijstand"/>
      <xsd:enumeration value="03 WW"/>
      <xsd:enumeration value="04 Wachtgeld"/>
      <xsd:enumeration value="05 RWW"/>
      <xsd:enumeration value="06 ANW"/>
      <xsd:enumeration value="07 Verzekering"/>
      <xsd:enumeration value="08 Wajong"/>
      <xsd:enumeration value="09 AOW"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AoSoortType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Vaste AO uitkering"/>
      <xsd:enumeration value="02 Tijdelijke AO uitkering"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="SoortVerzekeringType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Lijfrente verzekering"/>
      <xsd:enumeration value="02 AO verzekering"/>
      <xsd:enumeration value="03 Woonlasten verzekering"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="PensioenEntiteitType">
    <xsd:sequence>
      <xsd:element name="PensioenInkomen" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het (verwachte) pensioeninkomen bruto per jaar van de hypotheekgever (exclusief AOW, dat dient ingevoerd te worden onder de entiteit Uitkering).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="IngangsDtPensioen" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Geeft een exacte datum van ingang van het pensioen aan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EindDtPensioen" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Geeft de einddatum van het pensioen aan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="LandBelastingVerplichting" type="LandType">
        <xsd:annotation>
          <xsd:documentation>Land waarin belastingverplichting geldt op de gespecificeerde inkomsten uit pensioen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="OverigeEntiteitType">
    <xsd:sequence>
      <xsd:element name="SoortOverigeInkomsten" type="SoortOverigeInkomstenType">
        <xsd:annotation>
          <xsd:documentation>Geeft het soort overige inkomsten aan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SoortOverigeInkomstenOmschr" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>Vrije omschrijving van de overige inkomsten. Mag alleen gebruikt worden indien de omschrijving niet in de keuzelijst voorkomt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BrutoJaarBedragOverigeInkomsten" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Totaal bruto jaarbedrag van de overige inkomenssoort aan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EindDtOverigeInkomsten" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Geef de einddatum van de overige inkomsten aan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="SoortOverigeInkomstenType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Alimentatie"/>
      <xsd:enumeration value="02 Schenking"/>
      <xsd:enumeration value="03 BoxTwee"/>
      <xsd:enumeration value="99 Anders"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="VermogenEntiteitType">
    <xsd:sequence>
      <xsd:element name="SoortVermogen" type="SoortVermogenType">
        <xsd:annotation>
          <xsd:documentation>De aard van het vermogen waaruit inkomsten worden verkregen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BrutoJaarInkBoxDrie" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geeft het bedrag aan bruto jaarinkomen uit vermogen mee. Inkomsten die de consument vrij kan besteden zonder dat de vermogensbron wordt aangetast. Dit inkomen kan dienen ter bepaling van de maximale leencapaciteit (conform gedragscode CHF).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="IngangsDtBrutoJaarInkBoxDrie" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Geeft de ingangsdatum van de inkomsten in Box drie aan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EindDtBrutoJaarInkBoxDrie" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Geeft de eindatum van ontvangst van inkomsten uit box drie.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="LandBelastingVerplichting" type="LandType">
        <xsd:annotation>
          <xsd:documentation>Land waarin belastingverplichting geldt op de gespecificeerde inkomsten uit vermogen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PctInkomenMeetellen" type="N11_3Type">
        <xsd:annotation>
          <xsd:documentation>Geeft het percentage van het inkomen dat meegeteld moet worden ter bepaling van de leencapaciteit. Alleen opgeven als afwijkt van 100%.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DuurInMndMeetellen" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Geeft de specifieke looptijd in maanden waar een percentage van het inkomen moet meegeteld worden ter bepaling van de leencapaciteit. Alleen opgeven indien niet gelijk is aan duur hypotheek.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerpandVermogenJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de opgegeven inkomsten uit vermogen verpand zijn.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="SoortVermogenType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Sparen en Beleggen"/>
      <xsd:enumeration value="02 Inkomsten uit verhuur"/>
      <xsd:enumeration value="03 Anders"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="VerplichtingenEntiteitType">
    <xsd:sequence>
      <xsd:element name="VerplichtingLopendJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of het hier een nieuw af te sluiten consumptieve lening betreft die derhalve nog niet bekend is bij BKR maar wel meegenomen dient te worden in de risicobepaling.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="NrBestaandContract" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>Contract, rekening of overeenkomstnummer van het bestaande / lopende contract.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Maatschappij" type="MaatschappijType">
        <xsd:annotation>
          <xsd:documentation>Unieke HDN code van financiële instelling.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MaatschappijOmschr" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>Vrije omschrijving van de maatschappij niet voorkomend in de keuzelijst "MaatschappijType". Alleen vullen indien keuze niet voorkomt in keuzelijst.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Verplichting" type="VerplichtingType">
        <xsd:annotation>
          <xsd:documentation>Omschrijving van het soort verplichting.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerplichtingOmschr" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>Vrije omschrijving van het soort verplichting. Alleen vullen indien niet voorkomt in keuzelijst. Mag alleen gebruikt worden indien de omschrijving niet in de keuzelijst voorkomt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerplichtingPerJaar" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geeft het bruto bedrag dat per jaar aan de verplichting moet worden voldaan (aan rente en aflossing).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerplichtingBedrag" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geeft de hoogte van het actuele kredietbedrag (huidig schuldrestant) aan van het niet-doorlopend krediet.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="OorspronkelijkKredietBedrag" type="Bedrag">
        <xsd:annotation>
          <xsd:documentation>Het uitstaande bedrag van het lopende krediet bij aangaan van het nieuwe krediet.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="LimietDoorlopendKrediet" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het bedrag van de limiet van het doorlopend krediet.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerplichtingInMnd" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Aantal maanden dat de aanvrager verwacht nog aan de verplichting te moeten voldoen (resterende looptijd) Mag niet negatief zijn.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AflosBedrag" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geeft de hoogte van het af te lossen bedrag aan (inclusief evt. boete voor vervroegde aflossing)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AfgelostDoorHypotheek" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de verplichting zal worden afgelost door de nieuwe hypotheek (consumptief deel).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PctConsumptief" type="N11_3Type">
        <xsd:annotation>
          <xsd:documentation>Het percentage van het leenbedrag dat consumptief is (niet fiscaal aftrekbaar)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ExtraAfgelostBedrag" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het bedrag dat bovenop het reguliere aflosbedrag is terugbetaald (bijvoorbeeld bij afbetaling van studieschulden).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="VerplichtingType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Alimentatieverplichting t.g.v. kinderen"/>
      <xsd:enumeration value="02 Alimentatieverplichting t.g.v. partner"/>
      <xsd:enumeration value="03 Pensioenpremie t.l.v. partij"/>
      <xsd:enumeration value="04 Aflopend krediet of lening"/>
      <xsd:enumeration value="05 Doorlopend krediet"/>
      <xsd:enumeration value="06 Bestaande hypotheek"/>
      <xsd:enumeration value="07 Lening bij werkgever"/>
      <xsd:enumeration value="08 Verzendhuiskrediet"/>
      <xsd:enumeration value="09 Schuldregeling"/>
      <xsd:enumeration value="10 Telecommunicatievoorziening"/>
      <xsd:enumeration value="11 Bruto huurlasten"/>
      <xsd:enumeration value="12 Studielening"/>
      <xsd:enumeration value="13 Private Lease"/>
      <xsd:enumeration value="14 Studievoorschot"/>
      <xsd:enumeration value="99 Overig"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="Bedrag">
    <xsd:restriction base="xsd:decimal">
      <xsd:pattern value="-?\d{1,12}\.\d{2}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="BezittingenBoxDrieEntiteitType">
    <xsd:sequence>
      <xsd:element name="SoortBezittingBoxDrie" type="SoortBezittingBoxDrieType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan het soort bezittingen in box drie (voordeel uit sparen en beleggen)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SoortBezittingBoxDrieOmschr" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>Geeft een vrije omschrijving van de soort bezitting niet voorkomend in de keuzelijst soort bezitting box drie</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="WaardeBezittingBoxDrie" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Waarde bezitting box drie</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="SoortBezittingBoxDrieType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 banktegoeden"/>
      <xsd:enumeration value="02 girotegoeden"/>
      <xsd:enumeration value="03 spaartegoeden"/>
      <xsd:enumeration value="04 aandelen"/>
      <xsd:enumeration value="05 obligaties"/>
      <xsd:enumeration value="06 contant geld"/>
      <xsd:enumeration value="07 onroerend goed"/>
      <xsd:enumeration value="99 overige bezittingen"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="IdentificatieEntiteitType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="BSN" type="BSNType">
        <xsd:annotation>
          <xsd:documentation>Geeft het Burgerservicenummer weer. Minimaal 8 tekens, maximaal 9 tekens, voorloop nul is toegestaan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="LegitimatieSoort" type="LegitimatieSoortType">
        <xsd:annotation>
          <xsd:documentation>Het soort legitimatiebewijs</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="LegitimatieNr" type="AN20Type">
        <xsd:annotation>
          <xsd:documentation>Het nummer van het legitimatiebewijs</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="LegitimatieAfgifteDt" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Geeft de datum weer waarop het legitimatiedocument is afgegeven</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="LegitimatieAfgiftePlaats" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>Geeft de plaats weer waarop het legitimatiedocument is afgegeven</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="LegitimatieAfgifteLand" type="LandType">
        <xsd:annotation>
          <xsd:documentation>Geeft het land weer waar het legitimatiedocument is afgegeven. ISO Landenlijst 3166, versie 1 met aanvullend versie 3 voor vervallen gebieden. Noot: versie 1 bevat tweeletterige codes, versie 3 bevat vierletterige (!) codes.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="LegitimatieEindDt" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Geeft de datum weer tot wanneer het legitimatiedocument geldig is</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerblijfsVergunning" type="VerblijfsVergunningType">
        <xsd:annotation>
          <xsd:documentation>Het soort verblijfsvergunning</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerblijfsVergunningNr" type="AN20Type">
        <xsd:annotation>
          <xsd:documentation>Het nummer van de verblijfsvergunning.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerblijfsVergunningEindDt" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Einddatum van de verblijfsvergunning</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="optional"/>
  </xsd:complexType>
  <xsd:simpleType name="BSNType">
    <xsd:restriction base="xsd:nonNegativeInteger">
      <xsd:pattern value="[0-9]{8,9}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="LegitimatieSoortType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="EI Europese identiteitskaart"/>
      <xsd:enumeration value="PP Paspoort"/>
      <xsd:enumeration value="RB Rijbewijs"/>
      <xsd:enumeration value="VI Visum"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="VerblijfsVergunningType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 onbekend"/>
      <xsd:enumeration value="02 verblijfsvergunning onbepaalde tijd"/>
      <xsd:enumeration value="03 verblijfsvergunning bepaalde tijd"/>
      <xsd:enumeration value="04 verblijfsvergunning voor bepaalde tijd asiel (Document III)"/>
      <xsd:enumeration value="05 verblijfsvergunning voor onbepaalde tijd asiel (Document IV)"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="PartijNAWDataEntiteitType">
    <xsd:sequence>
      <xsd:element name="SoortPartij" type="SoortPartijType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of partij een natuurlijk persoon is of een rechtspersoon</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AchterNaam" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>De achternaam zonder tussenvoegsel. Bij natuurlijk persoon naam ingeven zoals ingeschreven bij GBA.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VoorNaam" type="AN35Type">
        <xsd:annotation>
          <xsd:documentation>Dit de betreft volledige eerste voornaam van de aanvrager</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TussenVoegsels" type="AN10Type">
        <xsd:annotation>
          <xsd:documentation>De tussenvoegsels behorende bij de achternaam.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="GeboorteNaamJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of er sprake is van een GeboorteAchternaam bij deze persoon</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="GebAchterNaam" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>De geboortenaam van de partij.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="GebTussenVoegsels" type="AN10Type">
        <xsd:annotation>
          <xsd:documentation>De tussenvoegsels behorende bij de geboortenaam.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Nationaliteit" type="LandType">
        <xsd:annotation>
          <xsd:documentation>Nationaliteit code (ISO landentabel 3166)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="GeboortePlaats" type="AN35Type">
        <xsd:annotation>
          <xsd:documentation>De geboorteplaats van de natuurlijke persoon.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="GeboorteLand" type="LandType">
        <xsd:annotation>
          <xsd:documentation>Het geboorteland van de natuurlijke persoon.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VoorLetters" type="AN15Type">
        <xsd:annotation>
          <xsd:documentation>Voorletter(s), in hoofdletters. Afwijkende voorletters als Th dienen letterlijk overgenomen te worden. Bijvoorbeeld: Theodoor Jan Carolus kan genoteerd worden als ThJC</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Geslacht" type="GeslachtType">
        <xsd:annotation>
          <xsd:documentation>Geslacht</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="GeboorteDt" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>De geboortdatum van de partij</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="StraatNaam" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>Straatnaam, indien sprake is van een correspondentie adres kan ook de aanduiding "postbus" gebruikt worden.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="HuisNr" type="N5Type">
        <xsd:annotation>
          <xsd:documentation>Het huisnummer, indien sprake is van een correspondentie adres kan dit ook een postbusnummer zijn.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="HuisNrToevoeging" type="AN10Type">
        <xsd:annotation>
          <xsd:documentation>Huisnummertoevoeging, bijvoorbeeld I, Huis, zwart etc.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Postcode" type="PostcodeType">
        <xsd:annotation>
          <xsd:documentation>De postcode, behorend bij ingevulde straatnaam / huisnummer of bij ingevuld postbusnummer. Voor Nederland 6 posities lang, zonder spatie ertussen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PlaatsNaam" type="AN35Type">
        <xsd:annotation>
          <xsd:documentation>De plaatsnaam.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Land" type="LandType">
        <xsd:annotation>
          <xsd:documentation>ISO landcode (ISO-3166)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VoorTitel" type="VoortitelType">
        <xsd:annotation>
          <xsd:documentation>Voortitel (b.v. ing)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RekeningNr" type="IBANType">
        <xsd:annotation>
          <xsd:documentation>Het IBAN rekeningnummer van de partij. Alleen hoofdletters toegestaan, invoeren zonder spaties.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
    <xsd:attribute name="ID" type="xsd:ID"/>
  </xsd:complexType>
  <xsd:simpleType name="SoortPartijType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 natuurlijk persoon"/>
      <xsd:enumeration value="02 rechtspersoon"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AN15Type">
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="15"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="GeslachtType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="M mannelijk"/>
      <xsd:enumeration value="V vrouwelijk"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="VoortitelType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 aalm"/>
      <xsd:enumeration value="02 adm"/>
      <xsd:enumeration value="03 bar"/>
      <xsd:enumeration value="04 biss"/>
      <xsd:enumeration value="05 bsse"/>
      <xsd:enumeration value="06 burg"/>
      <xsd:enumeration value="07 comm"/>
      <xsd:enumeration value="08 dkn"/>
      <xsd:enumeration value="09 dr"/>
      <xsd:enumeration value="10 dra"/>
      <xsd:enumeration value="11 dre"/>
      <xsd:enumeration value="12 drs"/>
      <xsd:enumeration value="13 ds"/>
      <xsd:enumeration value="14 gen"/>
      <xsd:enumeration value="15 gr"/>
      <xsd:enumeration value="16 htg"/>
      <xsd:enumeration value="17 ir"/>
      <xsd:enumeration value="18 jhr"/>
      <xsd:enumeration value="19 kan"/>
      <xsd:enumeration value="20 kap"/>
      <xsd:enumeration value="21 kapt"/>
      <xsd:enumeration value="22 kard"/>
      <xsd:enumeration value="23 kol"/>
      <xsd:enumeration value="24 kon"/>
      <xsd:enumeration value="25 kzr"/>
      <xsd:enumeration value="26 lt gen"/>
      <xsd:enumeration value="27 maj"/>
      <xsd:enumeration value="28 mgr"/>
      <xsd:enumeration value="29 min"/>
      <xsd:enumeration value="30 mr"/>
      <xsd:enumeration value="31 pasr"/>
      <xsd:enumeration value="32 pr"/>
      <xsd:enumeration value="33 pres"/>
      <xsd:enumeration value="34 prof"/>
      <xsd:enumeration value="35 ptr"/>
      <xsd:enumeration value="36 rect"/>
      <xsd:enumeration value="37 sinj"/>
      <xsd:enumeration value="38 weth"/>
      <xsd:enumeration value="39 ing"/>
      <xsd:enumeration value="40 da"/>
      <xsd:enumeration value="41 lt"/>
      <xsd:enumeration value="42 jvr"/>
      <xsd:enumeration value="43 bc"/>
      <xsd:enumeration value="44 bbsse"/>
      <xsd:enumeration value="45 fr"/>
      <xsd:enumeration value="46 frle"/>
      <xsd:enumeration value="47 jkvr"/>
      <xsd:enumeration value="48 rab"/>
      <xsd:enumeration value="49 wed"/>
      <xsd:enumeration value="50 zr"/>
      <xsd:enumeration value="51 dre"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="IBANType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Za-z]{2}[0-9]{2}[a-zA-Z0-9]{1,30}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="NotarisEntiteitType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="RefContactPersoonData" type="REFPartijNAWDataType">
        <xsd:annotation>
          <xsd:documentation>Geeft de naam van de entiteit waarin de NAW gegevens van de contactpersoon bij de notaris zijn opgenomen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BedrijfsNaam" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>De bedrijfsnaam.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CodeNotaris" type="NotarisType">
        <xsd:annotation>
          <xsd:documentation>Geeft de rol van notaris waarvan de gegevens worden doorgegeven.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TelefoonNrWerk" type="TelefoonNummerType">
        <xsd:annotation>
          <xsd:documentation>Telefoonnummer waarop betreffende persoon tijdens kantooruren te bereiken is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="NotarisType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="1 Passerend"/>
      <xsd:enumeration value="2 Transporterend"/>
      <xsd:enumeration value="3 Opsteller garantieverklaring"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="TussenPersoonEntiteitType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="RefContactPersoonData" type="REFPartijNAWDataType">
        <xsd:annotation>
          <xsd:documentation>Geeft de naam van de entiteit waarin de NAW gegevens van de contactpersoon bij de tussenpersoon zijn opgenomen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BedrijfsNaam" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>De bedrijfsnaam.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TussenpersoonNr" type="AN35Type">
        <xsd:annotation>
          <xsd:documentation>Nummer waaronder tussenpersoon geregistreerd is bij de maatschappij, waaraan de aanvraag gericht is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AanvullendeVerzendwijze" type="AanvullendeVerzendwijzeType">
        <xsd:annotation>
          <xsd:documentation>Geeft de aanvullende verzendwijze van de tussenpersoon weer, naast de verplichte HDN verzendwijze, op welke wijze het aangevraagde geretourneerd dient te worden. Per 1-5-2013 is verzending via HDN een verplichting.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TelefoonNrWerk" type="TelefoonNummerType">
        <xsd:annotation>
          <xsd:documentation>Telefoonnummer waarop betreffende persoon tijdens kantooruren te bereiken is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="E-mailadres" type="EmailType">
        <xsd:annotation>
          <xsd:documentation>Het E-mailadres in geval van retourneren per e-mail.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TelefoonNrMobiel" type="TelefoonNummerType">
        <xsd:annotation>
          <xsd:documentation>Mobiel nummer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AFMRegistratieNr" type="AN35Type">
        <xsd:annotation>
          <xsd:documentation>Het AFM nummer waaronder de bemiddelende tussenpersoon is ingeschreven zoals bedoeld in de WFT.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ServiceProvider" type="ServiceProviderEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Een {service provider} is een [rechtspersoon], die ondersteuning biedt aan [intermediairs] en kan werken namens een [aanbieder].</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="optional"/>
  </xsd:complexType>
  <xsd:simpleType name="AanvullendeVerzendwijzeType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Post"/>
      <xsd:enumeration value="02 Fax"/>
      <xsd:enumeration value="03 Email"/>
      <xsd:enumeration value="04 Extranet"/>
      <xsd:enumeration value="99 Anders"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="ServiceProviderEntiteitType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="RefContactPersoonData" type="REFPartijNAWDataType">
        <xsd:annotation>
          <xsd:documentation>Geeft de naam van de entiteit waarin de NAW gegevens van de contactpersoon zijn opgenomen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ServiceProviderNaam" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>ServiceProviderNaam</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ServiceProviderNr" type="AN35Type">
        <xsd:annotation>
          <xsd:documentation>ServiceProviderNr</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TelefoonNrWerk" type="TelefoonNummerType">
        <xsd:annotation>
          <xsd:documentation>Telefoonnummer waarop de partij per telefoon te bereiken is. (Alleen cijfers toegestaan, geen spatie of andere leestekens, 1 of 2 voorloopnullen toegestaan, geen beperking op aantal cijfers)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TelefoonNrMobiel" type="TelefoonNummerType">
        <xsd:annotation>
          <xsd:documentation>Telefoonnummer waarop de partij per telefoon te bereiken is. (Alleen cijfers toegestaan, geen spatie of andere leestekens, 1 of 2 voorloopnullen toegestaan, geen beperking op aantal cijfers)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="E-mailadres" type="EmailType">
        <xsd:annotation>
          <xsd:documentation>Emailadres van de partij</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ServiceProviderAFMRegistratieNr" type="AN35Type">
        <xsd:annotation>
          <xsd:documentation>ServiceProviderAFMRegistratieNr</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Internetadres" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>Het internetadres (homepage) van de tussenpersoon exclusief http://</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="optional"/>
  </xsd:complexType>
  <xsd:complexType name="ObjectEntiteitType">
    <xsd:sequence>
      <xsd:element name="CodeObject" type="CodeObjectType2">
        <xsd:annotation>
          <xsd:documentation>Indicatie betreffende het gebruiksdoel van het onderpand.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Onderpand" type="OnderpandType">
        <xsd:annotation>
          <xsd:documentation>Omschrijving van het soort onderpand.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="StraatNaam" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>Straatnaam</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="HuisNr" type="N5Type">
        <xsd:annotation>
          <xsd:documentation>Huisnummer</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="HuisNrToevoeging" type="AN10Type">
        <xsd:annotation>
          <xsd:documentation>Huisnummertoevoeging</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Postcode" type="PostcodeType">
        <xsd:annotation>
          <xsd:documentation>Postcode, behorend bij ingevulde straatnaam huisnummer of bij ingevuld postbusnummer. Voor Nederland 6 posities lang, zonder spatie ertussen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PlaatsNaam" type="AN35Type">
        <xsd:annotation>
          <xsd:documentation>Plaatsnaam</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Gemeente" type="AN30Type">
        <xsd:annotation>
          <xsd:documentation>Gemeente</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Land" type="LandType">
        <xsd:annotation>
          <xsd:documentation>ISO landcode (ISO-3166)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BouwPlanNr" type="AN35Type">
        <xsd:annotation>
          <xsd:documentation>Naam/nummer waaronder het project bij de projectontwikkelaar bekend is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SituatieNr" type="AN35Type">
        <xsd:annotation>
          <xsd:documentation>Nummer nieuwbouwwoning binnen het bouwplan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EigenBewoning" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of het object bedoeld is voor bewoning door de aanvrager(s).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CodeVerhuur" type="VerhuurType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of het object verhuurd gaat worden en onder welke conditie.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Huurinkomsten" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Bedrag dat jaarlijks aan huur wordt ontvangen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PercVerhuur" type="N11_3Type">
        <xsd:annotation>
          <xsd:documentation>Percentage van de totale vloeroppervlakte die wordt verhuurd.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EindVerhuurDt" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Datum die aangeeft wanneer de verhuur stopt</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AfkoopExPartner" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het deel van de waarde van de woning dat door de aanvrager aan de vertrekkende partner of erfgenamen moet worden vergoed conform notariële akte van levering ter uitvoering van de verdeling.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EigenaarBewonersAantal" type="N1Type">
        <xsd:annotation>
          <xsd:documentation>Aantal eigenaars/bewoners van het object.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Erfpacht" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of er sprake is van erfpacht.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ErfpachtEeuwigDurend" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de erfpacht eeuwigdurend is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ErfpachtBeginDt" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan vanaf welke datum de erfpacht loopt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ErfpachtEindDt" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan tot welke datum de erfpacht loopt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ErfpachtCanon" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Bedrag dat jaarlijks als vergoeding voor de erfpacht moet worden betaald.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ErfpachtAfkoopsom" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Bedrag van de af te kopen erfpacht.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ErfpachtMeeFinancieren" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de afkoopsom van de toekomstige erfpacht meegefinancierd wordt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ErfpachtPartJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of er sprake is van particuliere erfpacht.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SubsidieRegeling" type="SubsidieRegelingType">
        <xsd:annotation>
          <xsd:documentation>Geef aan of er sprake is van een subsidieregeling.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SubsidieRegelingBedrag" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geef hier het contante waarde bedrag van de subsidie op.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BouwJaar" type="N4Type">
        <xsd:annotation>
          <xsd:documentation>Jaar waarin het onderpand is opgeleverd.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AppartementsRecht" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of het appartementsrecht van kracht is op het onderpand.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BedragKwaliteitsverbeteringEBV" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geeft het bedrag dat besteed wordt aan energiebesparende maatregelen. Dit bedrag wordt niet meegenomen in de maandlastberekening t.b.v. financiering onder NHG en moet daarom apart (en afgesplitst) van het veld VerbouwingsKosten worden doorgegeven.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="HoofdVerblijf" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of het object als hoofdverblijf dient.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TaxatieAanwezig" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of er een (recent) taxatierapport aanwezig is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BronWaardebepaling" type="BronWaardebepalingType">
        <xsd:annotation>
          <xsd:documentation>De bron van de waardebepaling van het onderpand.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TaxatieDt" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>De datum van de taxatie.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="KostenTaxatie" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>De kosten van de taxatie.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Bouwaard" type="BouwaardType">
        <xsd:annotation>
          <xsd:documentation>Geeft de bouwaard van het object weer</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MarktWaarde" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>De (getaxeerde) marktwaarde van het onderpand.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Koopsom" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Bedrag van de koopsom (exclusief kosten koper en roerende zaken).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RoerendeZaken" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Bedrag van de roerende zaken.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="WOZWaarde" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Waarde van het onderpand in het kader van WOZ, incl. eventuele toeslag gemeente.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="WOZWaardeTaxatieDt" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Datum waardebepaling WOZ behorende bij de waarde in het veld WOZ waarde.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AchterstalligOnderhoud" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Bedrag dat de aanvrager verwacht te moeten betalen aan achterstallig onderhoud.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerbouwingsKosten" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geef het (geraamde) kostenbedrag benodigd voor de verbouwing exclusief het bedrag dat besteed wordt aan energiebesparende maatregelen. (Dat bedrag in veld BedragKwaliteitsverbeteringEBV invoeren)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MarktWaardeNaVerb" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>De (getaxeerde) marktwaarde van het onderpand na verbouwing.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerbouwingsDepot" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geef het bedrag aan dat in een (ver)bouwdepot gaat.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ZelfBouw" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de aanvrager de bouw van het onderpand zelf uitvoert.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="KoopsomGrond" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Bedrag van de koopsom van de grond van het onderpand.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RenteVerlies" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Bedrag aan renteverlies (= te betalen rente vanaf tekenen koopcontract tot aan oplevering) tijdens de bouw van een nieuw object.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Meerwerk" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Bedrag van kosten voor meerwerk (bij nieuwbouw).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="OpleveringsDt" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Verwachte datum waarop het onderpand wordt overgedragen aan koper.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BouwRente" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geeft het bedrag aan rente dat betaald moet worden aan de bouwer van het object voordat de akte gepaseerd is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ServiceKosten" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Bedrag van de maandelijkse servicekosten.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="KostenLeveringsAkteSpec" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geeft een gespecificeerd bedrag van de kosten voor de leverings- of overdrachtsakte.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MakelaarsKostenSpec" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geeft een gespecificeerd bedrag van de aankopende makelaarskosten.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="OverdrachtsBelastingSpec" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geeft een gespecificeerd bedrag van de overdrachtsbelasting.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerkoopOnderVoorwaardenJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan dat op het object een voorwaarde bij verkoop van toepassing is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EnergieKlasse" type="EnergieKlasseType">
        <xsd:annotation>
          <xsd:documentation>Geeft de energie (label) klasse in welke de woning valt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="KostenBouwkundigeKeuring" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Kosten van de bouwkundige keuring indien de bouwkundige keuring verplicht is gesteld door de geldverstrekker.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PremieOpstalverzekeringPerJaar" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Verzekeringspremie van de opstalverzekering indien opstalverzekering verplicht is gesteld door de geldverstrekker.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="9" name="KadastraleAanduiding" type="KadastraleAanduidingEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Kadastrale gegevens.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="optional"/>
    <xsd:attribute name="ID" type="xsd:ID"/>
  </xsd:complexType>
  <xsd:simpleType name="OnderpandType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 bestaand"/>
      <xsd:enumeration value="02 bestaand verbouw"/>
      <xsd:enumeration value="03 nieuw"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="N1Type">
    <xsd:restriction base="xsd:nonNegativeInteger">
      <xsd:pattern value="\d{1,1}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="SubsidieRegelingType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Koopsubsidie"/>
      <xsd:enumeration value="02 Starterslening"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="N4Type">
    <xsd:restriction base="xsd:nonNegativeInteger">
      <xsd:pattern value="\d{1,4}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="BronWaardebepalingType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Taxatierapport"/>
      <xsd:enumeration value="02 WOZ"/>
      <xsd:enumeration value="03 Koop overeenkomst"/>
      <xsd:enumeration value="04 Koop-/aanneem overeenkomst"/>
      <xsd:enumeration value="05 Geveltaxatie"/>
      <xsd:enumeration value="06 Stichtingskosten"/>
      <xsd:enumeration value="07 Modelmatige taxatie"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="BouwaardType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 (Grotendeels) steen"/>
      <xsd:enumeration value="02 Deels hout, deels steen"/>
      <xsd:enumeration value="03 (Grotendeels) hout"/>
      <xsd:enumeration value="04 Beton"/>
      <xsd:enumeration value="05 Staal"/>
      <xsd:enumeration value="99 Overig"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="EnergieKlasseType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 A++"/>
      <xsd:enumeration value="02 A+"/>
      <xsd:enumeration value="03 A"/>
      <xsd:enumeration value="04 B"/>
      <xsd:enumeration value="05 C"/>
      <xsd:enumeration value="06 D"/>
      <xsd:enumeration value="07 E"/>
      <xsd:enumeration value="08 F"/>
      <xsd:enumeration value="09 G"/>
      <xsd:enumeration value="10 A+++"/>
      <xsd:enumeration value="11 A++++"/>
      <xsd:enumeration value="12 Energie Neutraal"/>
      <xsd:enumeration value="13 Nul-op-de-Meter"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="KadastraleAanduidingEntiteitType">
    <xsd:sequence>
      <xsd:element name="Sectie" type="AN2Type">
        <xsd:annotation>
          <xsd:documentation>Aanduiding van kadastrale sectie van onderpand.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Nr" type="N15Type">
        <xsd:annotation>
          <xsd:documentation>Nummer waaronder onderpand geregistreerd is in het kadaster.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Grootte" type="N8Type">
        <xsd:annotation>
          <xsd:documentation>Oppervlakte van de grond waarop het onderpand staat en die behoort tot het onderpand (in vierkante meters).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="KadastraleGemeente" type="AN30Type">
        <xsd:annotation>
          <xsd:documentation>Aanduiding van gemeente volgens het kadaster.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="AN2Type">
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="2"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="N15Type">
    <xsd:restriction base="xsd:nonNegativeInteger">
      <xsd:pattern value="\d{1,15}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="LeningEntiteitType">
    <xsd:sequence>
      <xsd:element name="HypotheekBedrag" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Hoogte bedrag van gewenste hypotheek.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="HypothecaireInschrijving" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Gewenst bedrag van de hypothecaire inschrijving.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Financier" type="MaatschappijType">
        <xsd:annotation>
          <xsd:documentation>Naam van de financieringsinstelling die in het contract opgenomen dient te worden.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Regeling" type="RegelingType">
        <xsd:annotation>
          <xsd:documentation>Aanduiding of de aanvraag een nieuwe lening betreft of een vervolg op een bestaande lening is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="RangOrde" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Geeft de rangorde van de inschrijving weer bij het kadaster.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="NrBestaandeHyp" type="AN20Type">
        <xsd:annotation>
          <xsd:documentation>Het nummer van de bestaande hypotheek zoals dat in de administratie van de geldgever voorkomt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ProductNaam" type="AN35Type">
        <xsd:annotation>
          <xsd:documentation>Commerciële productnaam die door de geldverstrekker gebruikt wordt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CodeLeningMij" type="CodeLeningMijType">
        <xsd:annotation>
          <xsd:documentation>Code om lening te typeren zoals bekend bij maatschappij.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PasseerDt" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Datum waarop de lening (vermoedelijk) bij de notaris passeert.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="IngebEigenMiddelen" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Ingebracht bedrag aan eigen middelen ter verlaging van de hypotheek.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BankGarantieBedrag" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Bedrag gevraagde bankgarantie</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BankGarantieKosten" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Kosten van de bankgarantie</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VoorlopigeOfferteNr" type="AN35Type">
        <xsd:annotation>
          <xsd:documentation>Nummer van voorlopige offerte.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BespreekZaakJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan dat de ingediende aanvraag besproken met en/of voorgelegd wordt aan de ontvangende partij.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OfferteRente" type="OfferteRenteType">
        <xsd:annotation>
          <xsd:documentation>Wijze van vaststellen rente tussen moment offreren en passeren.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BetalingsmethodeAdvieskosten" type="AdvieskostenVormType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan op welke wijze de klant de advieskosten zal voldoen</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BrutoAdvieskosten" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geeft de standaard advieskosten die adviseur bij de klant in rekening kan brengen aan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="NettoAdvieskosten" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geeft het saldo aan van Bruto advieskosten minus het totaal aan verleende kortingen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BemiddelingsKosten" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Bemiddelingskosten welke rechtstreeks door het intermediair aan de klant gefactureerd worden.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Afhandelingskosten" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Bedrag aan kosten ten behoeve van afhandeling van de hypotheek</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BoeteRente" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het bedrag aan boeterente.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="NHG" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of het een lening onder NHG betreft.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Mutatie" type="MutatieType">
        <xsd:annotation>
          <xsd:documentation>Geeft het soort NHG melding aan. Dit veld alleen gebruiken voor aanvragen met NHG.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MaatwerkOplossing" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft weer of er sprake is van een hypothecaire aanvraag met een maatwerkoplossing volgens de normen van de CHF. (Comply or Explain, waarbij Comply = N Nee en Explain = J Ja)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="HypotheekAkteKostenSpec" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geeft een gespecificeerd bedrag van de kosten voor de hypotheekakte.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="NHGKostenSpec" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geeft een gespecificeerd bedrag voor de kosten NHG.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ComplexProductJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of het product een complex product (in de zin van de WFT) is</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AankoopWoningJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft weer of de lening wordt afgesloten ten behoeve van aankoop van een woning.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="9" name="MaatwerkOplSpec" type="MaatwerkOplSpecEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Geeft de specificatie van de maatwerkoplossing. (Explain)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="4" name="Leningdeel" type="LeningdeelEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Leningdeel</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="99" name="HypotheekOpties" type="HypotheekOptiesEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Gegevens over een structurele kortingsregeling. Tijdelijke actiekortingen e.d. dienen ingegeven te worden in de entiteit KortingsRegeling.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="optional"/>
  </xsd:complexType>
  <xsd:simpleType name="RegelingType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 eerste hypotheek (nieuw)"/>
      <xsd:enumeration value="04 onderhands"/>
      <xsd:enumeration value="05 omzetting"/>
      <xsd:enumeration value="07 interne oversluiting"/>
      <xsd:enumeration value="08 oversluiting andere geldgever"/>
      <xsd:enumeration value="09 tweede of hoger in rang hypotheek"/>
      <xsd:enumeration value="10 meeneemhypotheek"/>
      <xsd:enumeration value="11 doorgeefhypotheek"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CodeLeningMijType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="IN001 Reguliere hypotheek"/>
      <xsd:enumeration value="IN003 Watervilla hypotheek"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="OfferteRenteType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 dalrente"/>
      <xsd:enumeration value="02 offerterente"/>
      <xsd:enumeration value="03 toetsrente"/>
      <xsd:enumeration value="04 dagrente"/>
      <xsd:enumeration value="05 offerterente plus termijn"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="AdvieskostenVormType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Voldoen uit eigen middelen"/>
      <xsd:enumeration value="02 Meefinancieren in de hypotheek"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="MutatieType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Aankoop woning"/>
      <xsd:enumeration value="04 Verwerving uit onverdeelde boedel"/>
      <xsd:enumeration value="06 Oversluiting bij nieuwe financier"/>
      <xsd:enumeration value="07 Afkoop erfpacht"/>
      <xsd:enumeration value="27 Echtscheiding"/>
      <xsd:enumeration value="28 Kwaliteitsverbetering"/>
      <xsd:enumeration value="29 Verwerving volle eigendom"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="MaatwerkOplSpecEntiteitType">
    <xsd:sequence>
      <xsd:element name="MaatwerkOplCodeMij" type="MaatwerkOplCodeMijType">
        <xsd:annotation>
          <xsd:documentation>Geeft de maatschappij specifieke code voor de maatwerkoplossing weer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MaatwerkOplToelichting" type="AN255Type">
        <xsd:annotation>
          <xsd:documentation>Algemene toelichting met betrekking tot de maatwerkoplossing.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="MaatwerkOplCodeMijType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="IN001 Aantoonbare betaalcapaciteit op basis van historische woonlasten"/>
      <xsd:enumeration value="IN002 Inleg in premiedepot, lopende premievrije polis of inleg geheel uit eigen middelen"/>
      <xsd:enumeration value="IN004 Aantoonbaar vrij vermogen"/>
      <xsd:enumeration value="IN006 Aantoonbare inkomensstijging"/>
      <xsd:enumeration value="IN010 Ouders hoofdelijk medeschuldenaar"/>
      <xsd:enumeration value="IN099 Overige redenen"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="LeningdeelEntiteitType">
    <xsd:sequence>
      <xsd:element name="CodeLeningDeel" type="CodeLeningDeelType">
        <xsd:annotation>
          <xsd:documentation>Geeft de aard van het leningdeel.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BelastingBox" type="BelastingBoxType">
        <xsd:annotation>
          <xsd:documentation>Nummer van de belastingbox waarin het leningdeelbedrag valt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="NrBestaandeHyp" type="AN20Type">
        <xsd:annotation>
          <xsd:documentation>Leningnummer bestaande hypotheek(deel)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CodeDeelMij" type="CodeDeelMijType">
        <xsd:annotation>
          <xsd:documentation>Code om lening te typeren zoals bekend bij maatschappij</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DuurInMnd" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Looptijd in maanden, 999 = levenslang.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LeningDeelBedrag" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Bedrag van een afzonderlijk leningdeel.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AflossingsVorm" type="AflossingsVormType">
        <xsd:annotation>
          <xsd:documentation>De wijze van aflossing van het leningdeel.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CodeRenteMij" type="CodeRenteMijType">
        <xsd:annotation>
          <xsd:documentation>In dit veld geeft de maatschappij zijn specifieke naam aan voor de renteafspraak</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="RenteAfspraak" type="RenteAfspraakType">
        <xsd:annotation>
          <xsd:documentation>Geeft het soort renteafspraak aan dat voor dit leningdeel is gemaakt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="RenteVastInMnd" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Totaal aantal mnd waarin de rente contractueel is vastgelegd, inclusief mogelijke rbt. Waarbij de werkelijke contracttermijn doorgegeven wordt. Bijv. Dagrente =0, 1 mndsrente = 1, 3 mnd euribor = 3 en 10 jr met laatste 2 jr.rbt =120 enz.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RenteBedenkTijd" type="RenteBedenktijdType">
        <xsd:annotation>
          <xsd:documentation>Geeft de aard van de rentebedenktijd aan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RenteBedenkTijdInMnd" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Geeft in maanden de tijdsduur van de rentebedenktijd aan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BetalingsTermijn" type="BetalingsTermijnType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan met welke frequentie de rente en eventuele aflossing zal worden betaald.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BetalingAchteraf" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de termijnbetaling achteraf is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RentePct" type="N11_3Type">
        <xsd:annotation>
          <xsd:documentation>Het rentepercentage behorende bij de financiering.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RenteAfspraakOmschr" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>Omschrijving van de rente-afspraak. Alleen vullen indien waarde niet in keuzelijst voorkomt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RentePctBovenMarge" type="N11_3Type">
        <xsd:annotation>
          <xsd:documentation>De door de aanvrager gekozen bovenmarge percentage.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RentePctOnderMarge" type="N11_3Type">
        <xsd:annotation>
          <xsd:documentation>De door de aanvrager gekozen ondermarge percentage.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PctConsumptief" type="N11_3Type">
        <xsd:annotation>
          <xsd:documentation>Het percentage van het leningdeel bedrag dat consumptief is (niet fiscaal aftrekbaar)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ConsumptiefBedrag" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het bedragdeel van het leningdeel dat consumptief is (niet fiscaal aftrekbaar)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RenteAftrekEindDt" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Datum waarop de mogelijkheid tot renteaftrek komt te vervallen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="5" name="KortingsRegeling" type="KortingsRegelingEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Gegevens over een kortingsregeling. Structurele productkortingen e.d. dienen ingegeven te worden in de entiteit HypotheekOpties</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="9" name="FinancieleDekking" type="FinancieleDekkingEntiteitType1">
        <xsd:annotation>
          <xsd:documentation>Gegevens van verzekeringsproducten gekoppeld aan een leningdeel. Losse verzekeringsproducten dienen meegegeven te worden in de entiteit rechtreeks onder offerteaanvraag. (FinancieleDekking, AOV en WoonlastenVerzekering)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="9" name="BancaireDekking" type="BancaireDekkingEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Gegevens van een bancaire dekking (bijv. beleggingsrek., effectenrekening, spaarrekening) die als BEW, SEW enz. wordt gekoppeld aan een leningdeel.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="CodeLeningDeelType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="1 Nieuw"/>
      <xsd:enumeration value="2 Meeneem (zelfde geldgever, ander object)"/>
      <xsd:enumeration value="3 Doorgeef (Zelfde geldgever, zelfde  object, andere hypotheekgever)"/>
      <xsd:enumeration value="4 Omzetting (andere aflosvorm)"/>
      <xsd:enumeration value="5 Verhoging (hoger geldbedrag)"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="BelastingBoxType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Box 1"/>
      <xsd:enumeration value="03 Box 3"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CodeDeelMijType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="IN001 Aflossingsvrije Hypotheek"/>
      <xsd:enumeration value="IN002 Annuiteitenhypotheek"/>
      <xsd:enumeration value="IN003 Lineaire Hypotheek"/>
      <xsd:enumeration value="IN004 Maatwerkhypotheek"/>
      <xsd:enumeration value="IN005 Fondsenmix Hypotheek"/>
      <xsd:enumeration value="IN006 Levenhypotheek"/>
      <xsd:enumeration value="IN007 Beleggershypotheek"/>
      <xsd:enumeration value="IN008 Krediethypotheek"/>
      <xsd:enumeration value="IN009 Meerwaardehypotheek"/>
      <xsd:enumeration value="IN010 Spaarhypotheek"/>
      <xsd:enumeration value="IN011 Optimaalhypotheek"/>
      <xsd:enumeration value="IN012 Fondsenmix Hypotheek (Extra lenen)"/>
      <xsd:enumeration value="IN013 Beleggershypotheek (Extra lenen)"/>
      <xsd:enumeration value="IN014 Bankspaarhypotheek variant 1"/>
      <xsd:enumeration value="IN015 Bankspaarhypotheek variant 2"/>
      <xsd:enumeration value="IN016 Bankspaarhypotheek"/>
      <xsd:enumeration value="IN017 Restschuldfinanciering Annuitair"/>
      <xsd:enumeration value="IN018 Restschuldfinanciering Lineair"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CodeRenteMijType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="IN001 Middelrente"/>
      <xsd:enumeration value="IN002 Restant Rentevaste Periode"/>
      <xsd:enumeration value="IN003 Actuele Rente"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="RenteAfspraakType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 rentevast"/>
      <xsd:enumeration value="02 rentedempend systeem"/>
      <xsd:enumeration value="03 continu variabel"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="RenteBedenktijdType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 geen"/>
      <xsd:enumeration value="02 vooraf"/>
      <xsd:enumeration value="03 achteraf"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="BetalingsTermijnType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 per maand"/>
      <xsd:enumeration value="02 per 2 maanden"/>
      <xsd:enumeration value="03 per kwartaal"/>
      <xsd:enumeration value="06 per halfjaar"/>
      <xsd:enumeration value="12 per jaar"/>
      <xsd:enumeration value="99 eenmalig"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="KortingsRegelingEntiteitType">
    <xsd:sequence>
      <xsd:element name="KortingsSoort" type="AN20Type">
        <xsd:annotation>
          <xsd:documentation>Geeft het soort kortingsregeling aan. Ook commerciele afspraken kunnen hier doorgegeven worden.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="KortingsPct" type="N11_3Type">
        <xsd:annotation>
          <xsd:documentation>Korting op het rentepercentage gedurende de kortingslooptijd.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="KortingsBedrag" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het kortingsbedrag zoals b.v. rentekorting</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="KortingsDuurInMnd" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Looptijd kortingspercentage in maanden.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="FinancieleDekkingEntiteitType1">
    <xsd:sequence>
      <xsd:element name="CodeFinancieleDekking" type="CodeFinancieleDekkingType">
        <xsd:annotation>
          <xsd:documentation>Soort financiële dekking.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FiscaalGeruislozeVoortzettingJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of er sprake is van een fiscaal (geruisloze) voortzetting.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AanvangsDt" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>(geschatte) aanvangsdatum van de nieuwe dekking.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DuurInMnd" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Looptijd in maanden, 999 = levenslang</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ProductNaam" type="AN35Type">
        <xsd:annotation>
          <xsd:documentation>Commerciële naam van product</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Maatschappij" type="MaatschappijType">
        <xsd:annotation>
          <xsd:documentation>Unieke HDN code van financiële instelling.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MaatschappijOmschr" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>Vrije omschrijving van de maatschappij niet voorkomend in de keuzelijst "MaatschappijType". Alleen vullen indien keuze niet voorkomt in keuzelijst.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PolisLopendJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of er naast deze aanvraag nog een lopende polis bij deze of een andere verzekeraar is</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MaatschappijPolisLopend" type="MaatschappijType">
        <xsd:annotation>
          <xsd:documentation>Unieke code van financiële instelling</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MaatschappijPolisLopendOmschr" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>Vrije omschrijving van de maatschappij niet voorkomend in de keuzelijst "MaatschappijType". Alleen vullen indien keuze niet voorkomt in keuzelijst.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PolisNr" type="AN20Type">
        <xsd:annotation>
          <xsd:documentation>Polisnummer van de bestaande verzekeringsovereenkomst.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PolisAfkoopWaarde" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Afkoopwaarde van lopende polis welke wordt ingebracht in de aangevraagde levensverzekering. Het betreft de afkoop waarde op ingangsdatum van de nieuwe levensverzekering. Bij fiscale voortzetting is dit de overdrachtswaarde.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PolisOmzetting" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de aanvrager een lopende polis om wil zetten/inbrengen in een nieuw af te sluiten polis.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BerekeningsUitgangspunt" type="BerekeningsUitgangspuntType">
        <xsd:annotation>
          <xsd:documentation>Uitgangspunt voor de berekening van de verzekering.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="VerpandJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of financiële dekking verpand wordt</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="99" name="PremieAfspraken" type="PremieAfsprakenEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Gegevens van de premieafspraken. Inleg en onttrekkingen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="2" name="Verzekerde" type="VerzekerdeEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Gegevens over de verzekerde.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="2" name="VerzekeringNemer" type="VerzekeringNemerEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Gegevens over de Verzekeringnemer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="9" name="VerzekerdeBedragen" type="VerzekerdeBedragenEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Gegevens over de (verzekerde) dekkingen en gewenste (eind) kapitalen van de gekoppelde financiele dekking.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="99" name="FiscaalGeruislozeVoortzetting" type="FiscaalGeruislozeVoortzettingEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Geeft de gegevens van de fiscaal geruisloos voort te zetten dekking weer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="CodeFinancieleDekkingType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 levensverzekering"/>
      <xsd:enumeration value="04 (zelfstandige) risicoverzekering"/>
      <xsd:enumeration value="06 Erf/lijfrente"/>
      <xsd:enumeration value="07 Spaarverzekering"/>
      <xsd:enumeration value="08 Beleggingsverzekering"/>
      <xsd:enumeration value="09 Hybride verzekering"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="BerekeningsUitgangspuntType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Van premie naar kapitaal"/>
      <xsd:enumeration value="02 Van kapitaal naar premie"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="PremieAfsprakenEntiteitType">
    <xsd:sequence>
      <xsd:element name="SoortPremie" type="PremieType">
        <xsd:annotation>
          <xsd:documentation>Het soort premie.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AanvangNaIngangsDtInMnd" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Geschatte aanvangstermijn (in maanden) na ingangsdatum hypotheek.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DuurInMnd" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Duur in maanden.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PremieBedrag" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het premiebedrag.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BetalingsTermijn" type="BetalingsTermijnType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan met welke frequentie de premie/inleg zal worden betaald.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="RefDebiteurNAWData" type="REFPartijNAWDataType">
        <xsd:annotation>
          <xsd:documentation>Geeft de naam van de entiteit waarin de NAW gegevens zijn opgenomen van degene die verantwoordelijk is voor de betalingen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="PremieType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Risicopremie normaal"/>
      <xsd:enumeration value="03 Premie totaal normaal"/>
      <xsd:enumeration value="05 Storting"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="VerzekerdeEntiteitType">
    <xsd:sequence>
      <xsd:element name="RefPartijNAWData" type="REFPartijNAWDataType">
        <xsd:annotation>
          <xsd:documentation>Geeft de naam van de entiteit waarin de NAW gegevens van de verzekerde zijn opgenomen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BurgerlijkeStaat" type="BurgerlijkeStaatType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan wat de burgerlijke staat van de verzekerde op de ingangsdatum van de verzekering is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="RokerJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de verzekerde rookt of korter dan twee jaar geleden is gestopt met roken</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="VerzekeringNemerEntiteitType">
    <xsd:sequence>
      <xsd:element name="RefPartijNAWData" type="REFPartijNAWDataType">
        <xsd:annotation>
          <xsd:documentation>Geeft de naam van de entiteit waarin de NAW gegevens van de verzekeringnemer zijn opgenomen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BSN" type="BSNType">
        <xsd:annotation>
          <xsd:documentation>Geeft het Burgerservicenummer weer. Invoeren zonder scheidingstekens.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="VerzekerdeBedragenEntiteitType">
    <xsd:sequence>
      <xsd:element name="SoortVerzekerdKap" type="SoortVerzekerdKapType">
        <xsd:annotation>
          <xsd:documentation>Soort verzekerd kapitaal.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerzekerdKap" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het bij overlijden of bij expiratie minimaal uit te keren bedrag aan eerste verzekerde.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerzekerdKap2" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het bij overlijden of bij expiratie minimaal uit te keren bedrag aan tweede verzekerde.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BelastingBox" type="BelastingBoxType">
        <xsd:annotation>
          <xsd:documentation>Nummer van de belastingbox waarin het/de verzekerd(e) bedrag(en) valt/vallen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CodeUitkering" type="CodeUitkeringType">
        <xsd:annotation>
          <xsd:documentation>Type uitkering</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="SoortVerzekerdKapType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Verzekerd bedrag bij overlijden"/>
      <xsd:enumeration value="02 Bedrag bij leven/expiratie/prognosekapitaal"/>
      <xsd:enumeration value="03 Verzekerd percentage bij overlijden"/>
      <xsd:enumeration value="04 Som van de premies"/>
      <xsd:enumeration value="05 Contractwaarde"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="CodeUitkeringType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Lineair dalend"/>
      <xsd:enumeration value="02 annuitair dalend"/>
      <xsd:enumeration value="03 gelijkblijvend"/>
      <xsd:enumeration value="04 1-jarig (jaarlijks vast te stellen)"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="FiscaalGeruislozeVoortzettingEntiteitType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="DekkingNr" type="AN20Type">
        <xsd:annotation>
          <xsd:documentation>Kenmerk van de fiscaal geruisloos voort te zetten dekking.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AanvangsDtBox1" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Datum waarop het product (voor de eerste keer) een box1 product werd.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DekkingLopendBetalingLaag" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Hoogste som aan betalingen dat ooit in een jaar van de lopende dekking werd betaald.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DekkingLopendBetalingHoog" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Laagste som aan betalingen dat ooit in een jaar van de lopende dekking werd betaald.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BedragBetaaldLopendJaar" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Alle bedragen aan betalingen die sinds de laatste verjaardag tot ingangsdatum nieuwe dekking werden betaald.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CumulatieveBetalingEindDt" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Cumulatieve betaalde betalingen totale looptijd tot stortingsdatum in de nieuwe dekking.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DekkingOverdrachtsWaarde" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Overdrachtswaarde van de lopende dekking welke wordt ingebracht in de nieuwe dekking.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EerdereUitkeringDekking" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het bedrag uit een dekking dat eerder aan de aanvrager is uitgekeerd en welke de vrijstelling in box 1 heeft aangetast.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DuurInMnd" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>De oorspronkelijke duur van de fiscaal geruisloos voort te zetten dekking.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="BancaireDekkingEntiteitType">
    <xsd:sequence>
      <xsd:element name="Aanbieder" type="MaatschappijType">
        <xsd:annotation>
          <xsd:documentation>Geeft de naam van de aanbieder van het product weer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CodeBancaireDekking" type="CodeBancaireDekkingType">
        <xsd:annotation>
          <xsd:documentation>Geeft de soort bancaire dekking weer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FiscaalGeruislozeVoortzettingJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of er sprake is van een fiscaal (geruisloze) voortzetting.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AanvangsDt" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Geeft de (geschatte) aanvangsdatum van de dekking aan. In meeste gevallen gelijk aan de ingangsdatum van de overeenkomst.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DuurInMnd" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Geeft de contractsduur in maanden, 999 = levenslang.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ContractLopendJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of er sprake is van een reeds lopend contract.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ContractOmzettingJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de aanvrager het de waarde van het lopende contract in wil brengen in een nieuw af te sluiten contract.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ContractAfkoopWaarde" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Afkoopwaarde van het lopende contract welke ingebracht wordt in een nieuw af te sluiten dekking. Het betreft de afkoopwaarde op ingangsdatum van de nieuwe dekking. Bij fiscale voortzetting is dit de overdrachtswaarde.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ContractNr" type="AN20Type">
        <xsd:annotation>
          <xsd:documentation>Contract, rekening of overeenkomstnummer van het bestaande / lopende contract.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="99" name="InlegAfspraken" type="InlegAfsprakenEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Geeft de afspraken weer over inleg en eventuele ontrekkingen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="2" name="Contractant" type="ContractantEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Entiteit waar de gegevens van de contractant / rekeninghouder staan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="9" name="Waarden" type="WaardenEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Entiteit waar de gegevens over de (eind) waardes staan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="9" name="FiscaalGeruislozeVoortzetting" type="FiscaalGeruislozeVoortzettingEntiteitType1">
        <xsd:annotation>
          <xsd:documentation>Geeft de gegevens van de fiscaal geruisloos voort te zetten dekking weer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="CodeBancaireDekkingType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Spaarrekening"/>
      <xsd:enumeration value="02 Beleggingsrekening (alleen fondsen)"/>
      <xsd:enumeration value="03 Effectenrekening (losse aandelen)"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="InlegAfsprakenEntiteitType">
    <xsd:sequence>
      <xsd:element name="SoortInleg" type="InlegType">
        <xsd:annotation>
          <xsd:documentation>Geeft de soort inleg weer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AanvangNaIngangsDtInMnd" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Geschatte aanvangstermijn (in maanden) na ingangsdatum hypotheek.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DuurInMnd" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Duur in maanden.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="InlegBedrag" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het bedrag wat ingelegd wordt. Behorende bij de keuze in betalingstermijn.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BetalingsTermijn" type="BetalingsTermijnType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan met welke frequentie de inleg zal worden betaald of de onttrekking wordt gedaan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="RefDebiteurNAWData" type="REFPartijNAWDataType">
        <xsd:annotation>
          <xsd:documentation>Geeft de naam van de entiteit waarin de NAW gegevens zijn opgenomen van degene die verantwoordelijk is voor de specifieke InlegAfspraak.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="InlegType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Periodieke inleg"/>
      <xsd:enumeration value="02 Eenmalige inleg"/>
      <xsd:enumeration value="03 Periodieke onttrekking"/>
      <xsd:enumeration value="04 Eenmalige onttrekking"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="ContractantEntiteitType">
    <xsd:sequence>
      <xsd:element name="RefPartijNAWData" type="REFPartijNAWDataType">
        <xsd:annotation>
          <xsd:documentation>Geeft de naam/nummer van de entiteit waarin de NAW gegevens van de contractant zijn opgenomen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BSN" type="BSNType">
        <xsd:annotation>
          <xsd:documentation>Geeft het Burgerservicenummer weer. Invoeren zonder scheidingstekens.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="WaardenEntiteitType">
    <xsd:sequence>
      <xsd:element name="SoortWaarde" type="SoortWaardeType">
        <xsd:annotation>
          <xsd:documentation>Geeft de soort waarde aan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BedragWaarde" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Geeft het bedrag van de waarde.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BelastingBox" type="BelastingBoxType">
        <xsd:annotation>
          <xsd:documentation>Nummer van de belastingbox waarin de waarde valt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RekenRentePct" type="N11_3Type">
        <xsd:annotation>
          <xsd:documentation>Het percentage (reken) rente waarmee gerekend is voor het bedrag van de waarde. Indien er sprake is van beleggen, fondsen e.d dient de entiteit fondsverdeling gebruikt te worden.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="IngangsDtWaarde" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Geeft de datum van de waarde.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="SoortWaardeType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 Waarde op einddatum"/>
      <xsd:enumeration value="02 Waarde bij aanvang"/>
      <xsd:enumeration value="03 Contractwaarde op peildatum"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="FiscaalGeruislozeVoortzettingEntiteitType1">
    <xsd:sequence>
      <xsd:element name="FiscaleVormDekking" type="FiscaleVormType">
        <xsd:annotation>
          <xsd:documentation>Geeft de fiscale vorm van de FGV dekking aan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MaatschappijDekkingLopend" type="MaatschappijType">
        <xsd:annotation>
          <xsd:documentation>Unieke HDN code van financiele instelling waar huidige contract loopt.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MaatschappijDekkingLopendOmschr" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>Vrije omschrijving van de maatschappij niet voorkomend in de keuzelijst "MaatschappijType". Alleen vullen indien keuze niet voorkomt in keuzelijst.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DekkingNr" type="AN20Type">
        <xsd:annotation>
          <xsd:documentation>Kenmerk van de fiscaal geruisloos voort te zetten dekking.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OorspronkelijkeAanvangsDtLopendeDekking" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>De oorspronkelijke aanvangsdatum van de in te brengen lopende dekking ongeacht de fiscale box.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AanvangsDtBox1" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Datum waarop het product (voor de eerste keer) een box1 product werd.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="StortingDtNieuweDekking" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>Datum feitelijke storting van inbreng in de nieuwe dekking.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DekkingLopendBetalingLaag" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Laagste som aan betalingen dat ooit in een jaar van de lopende dekking werd betaald.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DekkingLopendBetalingHoog" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Hoogste som aan betalingen dat ooit in een jaar van de lopende dekking werd betaald.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BedragBetaaldLopendJaar" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Alle bedragen aan betalingen die sinds de laatste verjaardag tot ingangsdatum nieuwe dekking werden betaald.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DekkingOverdrachtsWaarde" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Overdrachtswaarde van de lopende dekking welke wordt ingebracht in de nieuwe dekking.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DuurInMnd" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>De oorspronkelijke duur van de fiscaal geruisloos voort te zetten dekking.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="FiscaleVormType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 SEW"/>
      <xsd:enumeration value="02 BEW"/>
      <xsd:enumeration value="03 KEW"/>
      <xsd:enumeration value="04 Kapitaalverzekering zonder eigen woningclausule"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="HypotheekOptiesEntiteitType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="CodeHypotheekOptiesMij" type="CodeHypotheekOptiesMijType">
        <xsd:annotation>
          <xsd:documentation>Geef hier de commerciële naam van de optie die gekozen is op de hypotheek.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="CodeHypotheekOptiesMijType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="IN012 ActieveBetaalKlant"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="FinancieleDekkingEntiteitType">
    <xsd:sequence>
      <xsd:element name="CodeFinancieleDekking" type="CodeFinancieleDekkingType">
        <xsd:annotation>
          <xsd:documentation>Soort financiële dekking.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="FiscaleVormDekking" type="FiscaleVormType">
        <xsd:annotation>
          <xsd:documentation>Geeft de vorm aan met betrekking tot het fiscaal voordelig vermogen opbouwen voor de aflossing van een hypotheek.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AanvangsDt" type="DatumType">
        <xsd:annotation>
          <xsd:documentation>(geschatte) aanvangsdatum</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DuurInMnd" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Looptijd in maanden, 999 = levenslang.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ProductNaam" type="AN35Type">
        <xsd:annotation>
          <xsd:documentation>Commerciële naam van product</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Maatschappij" type="MaatschappijType">
        <xsd:annotation>
          <xsd:documentation>Unieke HDN code van financiële instelling.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MaatschappijOmschr" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>Vrije omschrijving van de maatschappij niet voorkomend in de keuzelijst "MaatschappijType". Alleen vullen indien keuze niet voorkomt in keuzelijst.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PolisLopendJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of er naast deze aanvraag nog een lopende polis bij deze of een andere verzekeraar is</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MaatschappijPolisLopend" type="MaatschappijType">
        <xsd:annotation>
          <xsd:documentation>Unieke code van financiële instelling</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MaatschappijPolisLopendOmschr" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>Vrije omschrijving van de maatschappij niet voorkomend in de keuzelijst "MaatschappijType". Alleen vullen indien keuze niet voorkomt in keuzelijst.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PolisNr" type="AN20Type">
        <xsd:annotation>
          <xsd:documentation>Polisnummer van de bestaande verzekeringsovereenkomst.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PolisAfkoopWaarde" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Afkoopwaarde van lopende polis welke wordt ingebracht in de aangevraagde levensverzekering. Het betreft de afkoopwaarde op ingangsdatum van de nieuwe levensverzekering.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PolisOmzetting" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de aanvrager een lopende polis om wil zetten/inbrengen in een nieuw af te sluiten polis.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BerekeningsUitgangspunt" type="BerekeningsUitgangspuntType">
        <xsd:annotation>
          <xsd:documentation>Uitgangspunt voor de berekening van de verzekering.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PremieDepot" type="PremieDepotEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Gegevens van het premiedepot.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="99" name="PremieAfspraken" type="PremieAfsprakenEntiteitType1">
        <xsd:annotation>
          <xsd:documentation>Gegevens van de premieafspraken. Inleg en onttrekkingen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="10" name="FondsVerdeling" type="FondsVerdelingEntiteitType">
        <xsd:annotation>
          <xsd:documentation>Gegevens over (beleggings) fondsen. Ook bedoeld voor Hybrideproducten waarbij het spaardeel een specifiek fonds is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="2" name="Verzekerde" type="VerzekerdeEntiteitType1">
        <xsd:annotation>
          <xsd:documentation>Gegevens over de verzekerde.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="2" name="VerzekeringNemer" type="VerzekeringNemerEntiteitType1">
        <xsd:annotation>
          <xsd:documentation>VerzekeringNemer</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="9" name="VerzekerdeBedragen" type="VerzekerdeBedragenEntiteitType1">
        <xsd:annotation>
          <xsd:documentation>VerzekerdeBedragen</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="PremieDepotEntiteitType">
    <xsd:sequence>
      <xsd:element name="BedragDepot" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het bedrag aan premiedepot dat wordt ingebracht.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RentePct" type="N11_3Type">
        <xsd:annotation>
          <xsd:documentation>Rente percentage premiedepot</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="optional"/>
  </xsd:complexType>
  <xsd:complexType name="PremieAfsprakenEntiteitType1">
    <xsd:sequence>
      <xsd:element name="SoortPremie" type="PremieType">
        <xsd:annotation>
          <xsd:documentation>Het soort premie</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AanvangNaIngangsDtInMnd" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Geschatte aanvangstermijn (in maanden) na ingangsdatum hypotheek.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DuurInMnd" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Duur in maanden.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PremieBedrag" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het premiebedrag.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BetalingsTermijn" type="BetalingsTermijnType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan met welke frequentie de premie/inleg zal worden betaald.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="RefDebiteurNAWData" type="REFPartijNAWDataType">
        <xsd:annotation>
          <xsd:documentation>Geeft de naam van de entiteit waarin de NAW gegevens zijn opgenomen van degene die verantwoordelijk is voor de betalingen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PremieUitDepot" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het bedrag dat aan premie uit het depot moet worden betaald per betalingstermijn.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="FondsVerdelingEntiteitType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="FondsNaam" type="AN100Type">
        <xsd:annotation>
          <xsd:documentation>Naam van het fonds waarin wordt belegd</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PctFondsVerdeling" type="N11_3Type">
        <xsd:annotation>
          <xsd:documentation>Percentage van de totale inleg die in dit fonds wordt belegd</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="FondsCode" type="AN12Type">
        <xsd:annotation>
          <xsd:documentation>Code van het beleggingsfonds op basis van de ISIN-standaard</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="FondsType" type="FondsType1">
        <xsd:annotation>
          <xsd:documentation>Type beleggingsfonds</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:simpleType name="AN12Type">
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="12"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="FondsType1">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01 deposito"/>
      <xsd:enumeration value="02 obligaties"/>
      <xsd:enumeration value="03 vastgoed"/>
      <xsd:enumeration value="04 mix en overigen"/>
      <xsd:enumeration value="05 aandelen"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="VerzekerdeEntiteitType1">
    <xsd:sequence>
      <xsd:element name="RefPartijNAWData" type="REFPartijNAWDataType">
        <xsd:annotation>
          <xsd:documentation>Geeft de naam van de entiteit waarin de NAW gegevens van deze partij zijn opgenomen</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BurgerlijkeStaat" type="BurgerlijkeStaatType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan wat de huidige burgerlijke staat van de verzekerde is</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Beroep" type="BeroepType">
        <xsd:annotation>
          <xsd:documentation>(hoofd)beroep verzekerde</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="RokerJN" type="BooleanType">
        <xsd:annotation>
          <xsd:documentation>Geeft aan of de verzekerde rookt of korter dan twee jaar geleden is gestopt met roken</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="VerzekeringNemerEntiteitType1">
    <xsd:sequence>
      <xsd:element name="RefPartijNAWData" type="REFPartijNAWDataType">
        <xsd:annotation>
          <xsd:documentation>Naam en adresgegevens van de verzekeringnemer</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BSN" type="BSNType">
        <xsd:annotation>
          <xsd:documentation>Geeft het Burgerservicenummer weer. Invoeren zonder scheidingstekens.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="VerzekerdeBedragenEntiteitType1">
    <xsd:sequence>
      <xsd:element name="SoortVerzekerdKap" type="SoortVerzekerdKapType">
        <xsd:annotation>
          <xsd:documentation>Soort verzekerd kapitaal</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerzekerdKap" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het bij overlijden of bij expiratie minimaal uit te keren bedrag aan eerste verzekerde</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerzekerdKap2" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Het bij overlijden of bij expiratie minimaal uit te keren bedrag aan tweede verzekerde</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BelastingBox" type="BelastingBoxType">
        <xsd:annotation>
          <xsd:documentation>Nummer van de belastingbox waarin het/de verzekerd(e) bedrag(en) valt/vallen.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CodeUitkering" type="CodeUitkeringType">
        <xsd:annotation>
          <xsd:documentation>Type uitkering</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerzekerdPct" type="N11_3Type">
        <xsd:annotation>
          <xsd:documentation>Geeft het percentage van de poliswaarde dat als risicodekking wordt gehanteerd</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VerzekerdPct2" type="N11_3Type">
        <xsd:annotation>
          <xsd:documentation>Geeft het percentage van de poliswaarde dat als risicodekking wordt gehanteerd voor afwijkende percentages bij partner</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DuurInMnd" type="N3Type">
        <xsd:annotation>
          <xsd:documentation>Looptijd in maanden</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EindWaardeDekking" type="N12_2Type">
        <xsd:annotation>
          <xsd:documentation>Eindwaarde van de dekking</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="TekstRegelsEntiteitType">
    <xsd:sequence>
      <xsd:element name="TekstRegel" type="AN255Type">
        <xsd:annotation>
          <xsd:documentation>Vrije tekstregels</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TekstRegel2" type="AN255Type">
        <xsd:annotation>
          <xsd:documentation>Vrije tekstregels</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TekstRegel3" type="AN255Type">
        <xsd:annotation>
          <xsd:documentation>Vrije tekstregels</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TekstRegel4" type="AN255Type">
        <xsd:annotation>
          <xsd:documentation>Vrije tekstregels</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TekstRegel5" type="AN255Type">
        <xsd:annotation>
          <xsd:documentation>Vrije tekstregels</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Volgnummer" type="xsd:positiveInteger" use="optional"/>
  </xsd:complexType>
</xsd:schema>
